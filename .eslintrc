{
  "root": true,
  "env": {
    "node": true,
    "commonjs": true
  },
  "plugins": ["prettier", "vue"],
  "extends": [
    "plugin:vue/vue3-recommended",
    "plugin:prettier-vue/recommended",
    "prettier"
  ],
  "rules": {
    "no-debugger": "warn",
    "no-console": "warn",
    "comma-dangle": ["error", "always-multiline"]
  }
}
